{
    "version": "0.5",
    "name": "aiida_cp2k",
    "url": "https://github.com/cp2k/aiida-cp2k",
    "license": "MIT License",
    "author": "Ole Schütt, Aliaksandr Yakutovich, Patrick Seewald, Tiziano Müller, Andreas Glöss, Leonid Kahle",
    "author_email": "ole.schuett@empa.ch",
    "description": "A plugin to run CP2K calculations and workflows",
    "classifiers": [
        "License :: OSI Approved :: MIT License",
        "Programming Language :: Python :: 2.7",
        "Development Status :: 3 - Alpha"
    ],
    "install_requires": [
        "aiida",
        "ase"
    ],
    "entry_points": {
        "aiida.calculations": [
            "cp2k = aiida_cp2k.calculations:Cp2kCalculation"
        ],
        "aiida.parsers": [
            "cp2k = aiida_cp2k.parsers:Cp2kParser"
        ]
    },
    "data_files": [
        [".", ["setup.json"]]
    ]
}
